{
  "name": "douyu-ragpicker",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "npm run build-ng && npm run build-chrome",
    "build:prod": "npm run build-ng:prod && npm run build-chrome:prod",
    "build-ng": "ng build",
    "build-ng:watch": "ng build --watch",
    "build-ng:prod": "ng build --prod --aot",
    "build-chrome": "parcel build ./chrome/content-script.ts ./chrome/page-script.ts --no-minify --out-dir dist/douyu-ragpicker",
    "build-chrome:prod": "parcel build ./chrome/content-script.ts ./chrome/page-script.ts --out-dir dist/douyu-ragpicker",
    "build-chrome:watch": "parcel watch ./chrome/content-script.ts ./chrome/page-script.ts --out-dir dist/douyu-ragpicker"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "ng-zorro-antd": "^7.0.0-rc.0",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.4",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/chrome": "0.0.75",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "parcel-bundler": "^1.10.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
